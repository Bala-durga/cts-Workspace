package com.cts.slap.model;

import com.cts.slap.model.Student;
import com.cts.slap.exception.StudentLogException;

import java.util.List;

public interface StudentDAO{	
	String add(Student student) throws StudentLogException;
	boolean delete(int rollNum)throws StudentLogException;
	Student get(int rollNum) throws StudentLogException;;
	List<Student> getAll() throws StudentLogException;;
	boolean update(Student student) throws StudentLogException;
	
}

